{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "prettier",
    "react"
  ],
  "rules": {
    "constructor-super": 2,
    "for-direction": 2,
    "getter-return": 2,
    "no-async-promise-executor": 2,
    "no-case-declarations": 2,
    "no-class-assign": 2,
    "no-compare-neg-zero": 2,
    "no-cond-assign": 2,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-else-if": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-global-assign": 2,
    "no-import-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-misleading-character-class": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-new-symbol": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-prototype-builtins": 2,
    "no-regex-spaces": 2,
    "no-self-assign": 2,
    "no-setter-return": 2,
    "no-shadow-restricted-names": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "no-unused-labels": 2,
    "no-unused-vars": 2,
    "no-useless-catch": 2,
    "no-useless-escape": 2,
    "no-with": 2,
    "require-yield": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "brace-style": 2,
    "camelcase": 2,
    "comma-dangle": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "complexity": 2,
    "curly": 2,
    "func-name-matching": 2,
    "func-names": 2,
    "id-blacklist": 2,
    "id-denylist": 2,
    "id-length": 2,
    "indent": [
      2,
      2
    ],
    "indent-legacy": [
      2,
      2
    ],
    "jsx-quotes": [
      2,
      "prefer-single"
    ],
    "max-depth": 2,
    "max-params": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-implicit-coercion": 2,
    "no-empty-function": 2,
    "no-duplicate-imports": 2,
    "no-template-curly-in-string": 2,
    "no-unreachable-loop": 2,
    "no-unused-expressions": 2,
    "no-useless-return": 2,
    "object-curly-newline": 2,
    "object-curly-spacing": 2,
    "object-shorthand": 2,
    "prefer-object-spread": 2,
    "quotes": [
      2,
      "single"
    ],
    "semi-style": 2
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "process": true,
    "console": true,
    "document": false,
    "expect": false,
    "test": false
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "ignorePatterns": [
    "src/reportWebVitals.ts"
  ]
}